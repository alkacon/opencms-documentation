<?xml version="1.0" encoding="UTF-8"?>

<DocumentationDefinitionLists xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="opencms://system/modules/com.alkacon.opencms.documentation/schemas/documentation-definition-list.xsd">
  <DocumentationDefinitionList language="en">
    <Title><![CDATA[ResourceBean: Useful properties]]></Title>
    <Heading><![CDATA[List of useful properties]]></Heading>
    <PropertyDef>true</PropertyDef>
    <Entry>
      <Term><![CDATA[dateCreated]]></Term>
      <Description name="Description0">
        <links/>
        <content><![CDATA[<p>Returns the date of the creation of this resource.</p>]]></content>
      </Description>
    </Entry>
    <Entry>
      <Term><![CDATA[dateReleased]]></Term>
      <Description name="Description0">
        <links/>
        <content><![CDATA[<p>Returns the release date this resource.</p>]]></content>
      </Description>
    </Entry>
    <Entry>
      <Term><![CDATA[dateExpired]]></Term>
      <Description name="Description0">
        <links/>
        <content><![CDATA[<p>Returns the expiration date this resource.</p>]]></content>
      </Description>
    </Entry>
    <Entry>
      <Term><![CDATA[dateLastModified]]></Term>
      <Description name="Description0">
        <links/>
        <content><![CDATA[<p>Returns the date of the last modification of this resource.</p>]]></content>
      </Description>
    </Entry>
    <Entry>
      <Term><![CDATA[dateContent]]></Term>
      <Description name="Description0">
        <links/>
        <content><![CDATA[<p>Returns the date of the last modification of the content of this resource.</p>]]></content>
      </Description>
    </Entry>
    <Entry>
      <Term><![CDATA[name]]></Term>
      <Description name="Description0">
        <links/>
        <content><![CDATA[<p>Returns the file name of this resource without parent folders, for example “index.html”.</p>]]></content>
      </Description>
    </Entry>
  </DocumentationDefinitionList>
</DocumentationDefinitionLists>
